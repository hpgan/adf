{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Get_accessToken",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://api.ed-fi.org/v5.3/api/oauth/token",
					"method": "POST",
					"headers": {
						"Content-Type": "application/x-www-form-urlencoded",
						"Authorization": {
							"value": "'Basic ' + base64(pipeline().parameters.credential)",
							"type": "Expression"
						}
					},
					"body": "grant_type=client_credentials"
				}
			}
		],
		"parameters": {
			"credential": {
				"type": "String",
				"defaultValue": "RvcohKz9zHI4:E1iEFusaNf81xzCxwHfbolkC"
			}
		},
		"annotations": []
	}
}